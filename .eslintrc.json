{
    "parser": "babel-eslint",
    "env":{
        "browser": true,
        "node":true,
        "es6":true
    },
    "globals": {
    },
    "plugins": [
        "react-redux"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-redux/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "no-unused-vars":"off",
        "no-console":"off",
        "react-redux/connect-prefer-named-arguments": 2,
        "prettier/prettier": [
            "error",
            {
              "trailingComma": "es5"
            }
        ],
        "react/prop-types": [
            "enabled",
            { "ignore": "ignore", "customValidators": "customValidator" }
        ],
        "react-redux/prefer-separate-component-file": "off"
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    }
}
